<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssmTest.mapper.StudentMapper">
    <resultMap id="BaseResultMap" type="com.ssmTest.model.Student">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="student_number" jdbcType="BIGINT" property="studentNumber"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="major" jdbcType="VARCHAR" property="major"/>
        <result column="college" jdbcType="VARCHAR" property="college"/>
        <result column="birthday" jdbcType="BIGINT" property="birthday"/>
        <result column="sex" jdbcType="VARCHAR" property="sex"/>
        <result column="email" jdbcType="VARCHAR" property="email"/>
        <result column="account_id" jdbcType="VARCHAR" property="accountId"/>
    </resultMap>
    <select id="selectByAccountId" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        select * from student
        where account_id = #{accountId}
    </select>


</mapper>
